@prefix : <https://w3id.org/cocoon/v1.0#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vaem: <http://www.linkedmodel.org/schema/vaem> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <https://w3id.org/cocoon/v1.0> .

<https://w3id.org/cocoon/v1.0> rdf:type owl:Ontology ;
                                rdfs:seeAlso <https://github.com/miranda-zhang/cloud-computing-schema> ;
                                dc:issued "2018-09-20"^^xsd:date ;
                                dc:modified "2018-08-17"^^xsd:date ;
                                dc:title "Vocabulary in Cloud Computing Services"@en ;
                                owl:versionInfo 1.0 ;
                                dc:creator :mirandazhangqian ;
                                vann:preferredNamespaceUri "https://w3id.org/cocoon/v1.0#" ;
                                dc:description "Simple ontology for Cloud Computing Services. This ontology allows to define model of prices used in large cloud computing providers such as Google, Amazon, Azure, etc., including options for regions, type of instances, prices specification, etc. "@en ;
                                dc:rights "Copyright © 2018 Miranda ZHANG Qian" ;
                                vann:preferredNamespacePrefix "cocoon" ;
                                dc:created "2012-10-01"^^xsd:date ;
                                rdfs:label "Cloud Computing Services Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/goodrelations/v1#name
gr:name rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/mbox
foaf:mbox rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/page
foaf:page rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/goodrelations/v1#hasPriceSpecification
gr:hasPriceSpecification rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#canHaveSnapshot
:canHaveSnapshot rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasApplianceType
:hasApplianceType rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasBandwidth
:hasBandwidth rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasBenchmark
:hasBenchmark rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasCPUcapacity
:hasCPUcapacity rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasCapacity
:hasCapacity rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasClockRate
:hasClockRate rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasFunctionality
:hasFunctionality rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasMaxNumberOfDisk
:hasMaxNumberOfDisk rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasStorage .


###  https://w3id.org/cocoon/v1.0#hasMaxStorageSize
:hasMaxStorageSize rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasStorage .


###  https://w3id.org/cocoon/v1.0#hasMemory
:hasMemory rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasMetric
:hasMetric rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasOperatingSystem
:hasOperatingSystem rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasOrchestrationInterface
:hasOrchestrationInterface rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasProvider
:hasProvider rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasQuantity
:hasQuantity rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasRegion
:hasRegion rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf gr:hasPriceSpecification .


###  https://w3id.org/cocoon/v1.0#hasScript
:hasScript rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasServiceLevelAgreement
:hasServiceLevelAgreement rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasStorage
:hasStorage rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#hasStorageIOMax
:hasStorageIOMax rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasStorage .


###  https://w3id.org/cocoon/v1.0#hasStorageSize
:hasStorageSize rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasStorage .


###  https://w3id.org/cocoon/v1.0#hasStorageThroughputMax
:hasStorageThroughputMax rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasStorage .


###  https://w3id.org/cocoon/v1.0#hasUnitOfMeasurement
:hasUnitOfMeasurement rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#inJurisdiction
:inJurisdiction rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#inPhysicalLocation
:inPhysicalLocation rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#isSoftware
:isSoftware rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#isSoftwareType
:isSoftwareType rdf:type owl:ObjectProperty .


###  https://w3id.org/cocoon/v1.0#ofObjectType
:ofObjectType rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/cocoon/v1.0#CPUClockSpeed
:CPUClockSpeed rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#CPUFamily
:CPUFamily rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#bandwidth
:bandwidth rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#chargedPerCore
:chargedPerCore rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :numberOfCores ;
                rdfs:comment "For Google Cloud, cost of SQL server image is charged on per core basis."@en .


###  https://w3id.org/cocoon/v1.0#continent
:continent rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :location ;
           rdfs:comment "Similar to location, this property specifies the continent."@en .


###  https://w3id.org/cocoon/v1.0#forCoresLessEqual
:forCoresLessEqual rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :numberOfCores ;
                   rdfs:comment "Google Cloud OS price for VM with different number of cores are different."@en .


###  https://w3id.org/cocoon/v1.0#forCoresMoreThan
:forCoresMoreThan rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :numberOfCores ;
                  rdfs:comment "Google Cloud OS price for VM with different number of cores are different."@en .


###  https://w3id.org/cocoon/v1.0#genericName
:genericName rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#hash
:hash rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#latency
:latency rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:comment "The human-readable name for the actual location of the data center in a region, this is the original value used to generate the linking to Geoname ontology."@en .


###  https://w3id.org/cocoon/v1.0#method
:method rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#numberOfCores
:numberOfCores rdf:type owl:DatatypeProperty ;
               rdfs:comment "For Google Cloud, cores and vCPU referes to the same thing." .


###  https://w3id.org/cocoon/v1.0#numericValue
:numericValue rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#persistence
:persistence rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#requestPayload
:requestPayload rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#resourceRecord
:resourceRecord rdf:type owl:DatatypeProperty .


###  https://w3id.org/cocoon/v1.0#responsePayload
:responsePayload rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/goodrelations/v1#BusinessEntity
gr:BusinessEntity rdf:type owl:Class .


###  http://purl.org/goodrelations/v1#UnitPriceSpecification
gr:UnitPriceSpecification rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty :hasRegion ;
                                            owl:someValuesFrom :Region
                                          ] .


###  http://qudt.org/schema/qudt#DataRateUnit
<http://qudt.org/schema/qudt#DataRateUnit> rdf:type owl:Class .


###  http://qudt.org/schema/qudt#DerivedUnit
<http://qudt.org/schema/qudt#DerivedUnit> rdf:type owl:Class .


###  http://qudt.org/schema/qudt#InformationEntropyUnit
<http://qudt.org/schema/qudt#InformationEntropyUnit> rdf:type owl:Class .


###  http://qudt.org/schema/qudt#Unit
<http://qudt.org/schema/qudt#Unit> rdf:type owl:Class ;
                                   owl:equivalentClass :UnitOfMeasure .


###  http://www.geonames.org/ontology#Feature
<http://www.geonames.org/ontology#Feature> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#API
:API rdf:type owl:Class ;
     rdfs:subClassOf :Software .


###  https://w3id.org/cocoon/v1.0#Appliance
:Appliance rdf:type owl:Class ;
           rdfs:subClassOf :PaaS ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasApplianceType ;
                             owl:someValuesFrom :SaaS
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :genericName ;
                             owl:someValuesFrom xsd:string
                           ] .


###  https://w3id.org/cocoon/v1.0#Application
:Application rdf:type owl:Class ;
             rdfs:subClassOf :SoftwareType .


###  https://w3id.org/cocoon/v1.0#BLOBStorage
:BLOBStorage rdf:type owl:Class ;
             rdfs:subClassOf :NetworkStorage .


###  https://w3id.org/cocoon/v1.0#BlockDeviceStorage
:BlockDeviceStorage rdf:type owl:Class ;
                    rdfs:subClassOf :NetworkStorage .


###  https://w3id.org/cocoon/v1.0#CPU
:CPU rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasBenchmark ;
                       owl:someValuesFrom :QuantityOfThings
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasClockRate ;
                       owl:someValuesFrom :QuantityOfThings
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :CPUFamily ;
                       owl:someValuesFrom xsd:string
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :numberOfCores ;
                       owl:someValuesFrom xsd:decimal
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :resourceRecord ;
                       owl:someValuesFrom xsd:anyURI
                     ] .


###  https://w3id.org/cocoon/v1.0#CloudService
:CloudService rdf:type owl:Class ;
              rdfs:subClassOf :Service ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gr:hasPriceSpecification ;
                                owl:someValuesFrom gr:UnitPriceSpecification
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasProvider ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass gr:BusinessEntity
                              ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CloudService ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty gr:hasPriceSpecification ;
                         owl:someValuesFrom gr:UnitPriceSpecification
                       ] ;
   rdfs:comment "Price specification for Cloud Service"@en
 ] .


###  https://w3id.org/cocoon/v1.0#ClusteredCPU
:ClusteredCPU rdf:type owl:Class ;
              rdfs:subClassOf :CPU ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :CPUClockSpeed ;
                                owl:someValuesFrom xsd:decimal
                              ] .


###  https://w3id.org/cocoon/v1.0#CommunityCloud
:CommunityCloud rdf:type owl:Class ;
                rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/v1.0#CustomizableAppliance
:CustomizableAppliance rdf:type owl:Class ;
                       rdfs:subClassOf :Appliance ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasScript ;
                                         owl:someValuesFrom :Script
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasOperatingSystem ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :SystemImage
                                       ] .


###  https://w3id.org/cocoon/v1.0#DeploymentModel
:DeploymentModel rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#HybridCloud
:HybridCloud rdf:type owl:Class ;
             rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/v1.0#IaaS
:IaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService .


###  https://w3id.org/cocoon/v1.0#Interface
:Interface rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#Key
:Key rdf:type owl:Class ;
     rdfs:subClassOf :API .


###  https://w3id.org/cocoon/v1.0#LocalStorage
:LocalStorage rdf:type owl:Class ;
              rdfs:subClassOf :Storage ;
              rdfs:comment "Storage that is not detachable from VM"@en .


###  https://w3id.org/cocoon/v1.0#Location
:Location rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#Metric
:Metric rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasQuantity ;
                          owl:someValuesFrom :QuantityOfThings
                        ] .


###  https://w3id.org/cocoon/v1.0#Name
:Name rdf:type owl:Class ;
      rdfs:subClassOf :API .


###  https://w3id.org/cocoon/v1.0#Network
:Network rdf:type owl:Class ;
         rdfs:subClassOf :IaaS ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasBandwidth ;
                           owl:someValuesFrom :QuantityOfThings
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :bandwidth ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :latency ;
                           owl:someValuesFrom xsd:string
                         ] .


###  https://w3id.org/cocoon/v1.0#NetworkStorage
:NetworkStorage rdf:type owl:Class ;
                rdfs:subClassOf :Storage ;
                rdfs:comment "Storage can exist independent from VM"@en .


###  https://w3id.org/cocoon/v1.0#OSPriceSpecification
:OSPriceSpecification rdf:type owl:Class ;
                      rdfs:subClassOf gr:UnitPriceSpecification ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :chargedPerCore ;
                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:boolean
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :forCoresLessEqual ;
                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:decimal
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :forCoresMoreThan ;
                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:decimal
                                      ] .


###  https://w3id.org/cocoon/v1.0#PaaS
:PaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService .


###  https://w3id.org/cocoon/v1.0#Password
:Password rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hash ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] .


###  https://w3id.org/cocoon/v1.0#PrivateCloud
:PrivateCloud rdf:type owl:Class ;
              rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/v1.0#PublicCloud
:PublicCloud rdf:type owl:Class ;
             rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/v1.0#QualityOfService
:QualityOfService rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#QuantityOfThings
:QuantityOfThings rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasUnitOfMeasurement ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :UnitOfMeasure
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :ofObjectType ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :numericValue ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:double
                                  ] ;
                  rdfs:comment "Quantify things, i.e. 3 TB of SSD Storage, 8 GB of Memory" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :QuantityOfThings ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :ofObjectType ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                       ] ;
   rdfs:comment """Optional, if not sure the exact type of thing,
like only know it is 8GB of Memory, but there is not a definition in ontology for the exact memory type.
One usage example, `cocoon:ofObjectType cocoon:LocalStorage` ,
quantifies the type of object should be `LocalStorage`"""@en
 ] .


###  https://w3id.org/cocoon/v1.0#REST
:REST rdf:type owl:Class ;
      rdfs:subClassOf :Interface ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :method ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :requestPayload ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :responsePayload ;
                        owl:someValuesFrom xsd:string
                      ] .


###  https://w3id.org/cocoon/v1.0#Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf :Location ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :inJurisdiction ;
                          owl:someValuesFrom <http://www.geonames.org/ontology#Feature>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasProvider ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass gr:BusinessEntity
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :inPhysicalLocation ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass <http://www.geonames.org/ontology#Feature>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :continent ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :location ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Region ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :inJurisdiction ;
                         owl:someValuesFrom <http://www.geonames.org/ontology#Feature>
                       ] ;
   rdfs:comment "Some regions can be in multiple locations, i.e. nam-eur-asia1, they can be in multiple jurisdictions. Other region should have at most 1 phisical location associated with it."
 ] .


###  https://w3id.org/cocoon/v1.0#SaaS
:SaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isSoftware ;
                        owl:someValuesFrom :Software
                      ] .


###  https://w3id.org/cocoon/v1.0#Script
:Script rdf:type owl:Class ;
        rdfs:subClassOf :SoftwareType .


###  https://w3id.org/cocoon/v1.0#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasOrchestrationInterface ;
                           owl:someValuesFrom :Interface
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasServiceLevelAgreement ;
                           owl:someValuesFrom :ServiceLevelAgreement
                         ] .


###  https://w3id.org/cocoon/v1.0#ServiceLevelAgreement
:ServiceLevelAgreement rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#Software
:Software rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasFunctionality ;
                            owl:someValuesFrom :SoftwareFunctionality
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isSoftwareType ;
                            owl:someValuesFrom :SoftwareType
                          ] .


###  https://w3id.org/cocoon/v1.0#SoftwareFunctionality
:SoftwareFunctionality rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#SoftwareType
:SoftwareType rdf:type owl:Class .


###  https://w3id.org/cocoon/v1.0#Storage
:Storage rdf:type owl:Class ;
         rdfs:subClassOf :IaaS ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :canHaveSnapshot ;
                           owl:someValuesFrom :Storage
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasStorageIOMax ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :QuantityOfThings
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasStorageSize ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :QuantityOfThings
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasStorageThroughputMax ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :QuantityOfThings
                         ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Storage ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :canHaveSnapshot ;
                         owl:someValuesFrom :Storage
                       ] ;
   rdfs:comment "Available snapshot options"@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Storage ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :hasStorageIOMax ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :QuantityOfThings
                       ] ;
   rdfs:comment "Disks can have different input/output operations per sec (IOPs)."@en
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Storage ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :hasStorageThroughputMax ;
