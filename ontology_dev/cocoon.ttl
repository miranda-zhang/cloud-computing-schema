@base <https://w3id.org/cocoon/v1.0> .

<https://w3id.org/cocoon/v1.0> rdf:type owl:Ontology ;
                                rdfs:seeAlso <https://github.com/miranda-zhang/cloud-computing-schema> ;
                                dc:issued "2018-09-20"^^xsd:date ;
                                dc:modified "2018-08-17"^^xsd:date ;
                                dc:title "Vocabulary in Cloud Computing Services"@en ;
                                owl:versionInfo 1.0 ;
                                dc:creator :mirandazhangqian ;
                                vann:preferredNamespaceUri "https://w3id.org/cocoon/v1.0#" ;
                                dc:description "Simple ontology for Cloud Computing Services. This ontology allows to define model of prices used in large cloud computing providers such as Google, Amazon, Azure, etc., including options for regions, type of instances, prices specification, etc. "@en ;
                                dc:rights "Copyright © 2018 Miranda ZHANG Qian" ;
                                vann:preferredNamespacePrefix "cocoon" ;
                                dc:created "2012-10-01"^^xsd:date ;
                                rdfs:label "Cloud Computing Services Ontology"@en .

