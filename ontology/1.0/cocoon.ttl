#################################################################
#    Classes
#################################################################

###  http://purl.org/goodrelations/v1#UnitPriceSpecification
gr:UnitPriceSpecification rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


###  https://w3id.org/cocoon/def#API
:API rdf:type owl:Class ;
     rdfs:subClassOf :Software .


###  https://w3id.org/cocoon/def#AccessControl
:AccessControl rdf:type owl:Class .


###  https://w3id.org/cocoon/def#Appliance
:Appliance rdf:type owl:Class ;
           rdfs:subClassOf :PaaS ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasApplianceType ;
                             owl:someValuesFrom :SaaS
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :genericName ;
                             owl:someValuesFrom xsd:string
                           ] .


###  https://w3id.org/cocoon/def#Application
:Application rdf:type owl:Class ;
             rdfs:subClassOf :SoftwareType .


###  https://w3id.org/cocoon/def#Attribute
:Attribute rdf:type owl:Class .


###  https://w3id.org/cocoon/def#BLOBStorage
:BLOBStorage rdf:type owl:Class ;
             rdfs:subClassOf :NetworkStorage .


###  https://w3id.org/cocoon/def#BlockDeviceStorage
:BlockDeviceStorage rdf:type owl:Class ;
                    rdfs:subClassOf :NetworkStorage .


###  https://w3id.org/cocoon/def#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :onChip ;
                       owl:someValuesFrom xsd:boolean
                     ] .


###  https://w3id.org/cocoon/def#CIFS
:CIFS rdf:type owl:Class ;
      rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#CPU
:CPU rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :hasBenchmark ;
                       owl:someValuesFrom :PhysicalQuantity
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasClockRate ;
                       owl:someValuesFrom :PhysicalQuantity
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasLocalStorage ;
                       owl:someValuesFrom :LocalStorage
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :CPUFamily ;
                       owl:someValuesFrom xsd:string
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :numberOfCores ;
                       owl:someValuesFrom xsd:decimal
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :resourceRecord ;
                       owl:someValuesFrom xsd:anyURI
                     ] .


###  https://w3id.org/cocoon/def#CloudService
:CloudService rdf:type owl:Class ;
              rdfs:subClassOf :Service ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gr:hasPriceSpecification ;
                                owl:someValuesFrom gr:UnitPriceSpecification
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasCredential ;
                                owl:someValuesFrom :Credential
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDeploymentModel ;
                                owl:someValuesFrom :DeploymentModel
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasPhysicalLocation ;
                                owl:someValuesFrom :Location
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasProvider ;
                                owl:someValuesFrom :Provider
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasRegion ;
                                owl:someValuesFrom :Region
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isAvailableIn ;
                                owl:someValuesFrom :Location
                              ] .


###  https://w3id.org/cocoon/def#ClusteredCPU
:ClusteredCPU rdf:type owl:Class ;
              rdfs:subClassOf :CPU ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasCommunicationBus ;
                                owl:someValuesFrom :Bus
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :CPUClockSpeed ;
                                owl:someValuesFrom xsd:decimal
                              ] .


###  https://w3id.org/cocoon/def#CommunityCloud
:CommunityCloud rdf:type owl:Class ;
                rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/def#Credential
:Credential rdf:type owl:Class ;
            rdfs:subClassOf :AccessControl ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPassword ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Password
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasUser ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :User
                            ] .


###  https://w3id.org/cocoon/def#CustomizableAppliance
:CustomizableAppliance rdf:type owl:Class ;
                       rdfs:subClassOf :Appliance ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasScript ;
                                         owl:someValuesFrom :Script
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasVirtualizationFormat ;
                                         owl:someValuesFrom :VirtualizationFormat
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasOperatingSystem ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :OperatingSystem
                                       ] .


###  https://w3id.org/cocoon/def#DeploymentModel
:DeploymentModel rdf:type owl:Class .


###  https://w3id.org/cocoon/def#FTP
:FTP rdf:type owl:Class ;
     rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#HTTP
:HTTP rdf:type owl:Class ;
      rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#HybridCloud
:HybridCloud rdf:type owl:Class ;
             rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/def#IP
:IP rdf:type owl:Class ;
    rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#IPv4
:IPv4 rdf:type owl:Class ;
      rdfs:subClassOf :IP .


###  https://w3id.org/cocoon/def#IPv6
:IPv6 rdf:type owl:Class ;
      rdfs:subClassOf :IP .


###  https://w3id.org/cocoon/def#IaaS
:IaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasStorage ;
                        owl:someValuesFrom :Storage
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :fileSystem ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :persistence ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :storageFormat ;
                        owl:someValuesFrom xsd:decimal
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :storageSizeMax ;
                        owl:someValuesFrom xsd:decimal
                      ] .


###  https://w3id.org/cocoon/def#Input
:Input rdf:type owl:Class ;
       rdfs:subClassOf :Operation .


###  https://w3id.org/cocoon/def#Interface
:Interface rdf:type owl:Class .


###  https://w3id.org/cocoon/def#Key
:Key rdf:type owl:Class ;
     rdfs:subClassOf :API .


###  https://w3id.org/cocoon/def#LocalStorage
:LocalStorage rdf:type owl:Class ;
              rdfs:subClassOf :VM .


###  https://w3id.org/cocoon/def#Location
:Location rdf:type owl:Class .


###  https://w3id.org/cocoon/def#MeasurableAttribute
:MeasurableAttribute rdf:type owl:Class ;
                     rdfs:subClassOf :Attribute ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasMetric ;
                                       owl:someValuesFrom :Metric
                                     ] .


###  https://w3id.org/cocoon/def#Metric
:Metric rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasQuantity ;
                          owl:someValuesFrom :PhysicalQuantity
                        ] .


###  https://w3id.org/cocoon/def#Name
:Name rdf:type owl:Class ;
      rdfs:subClassOf :API .


###  https://w3id.org/cocoon/def#Network
:Network rdf:type owl:Class ;
         rdfs:subClassOf :IaaS ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasBandwidth ;
                           owl:someValuesFrom :PhysicalQuantity
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :bandwidth ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :latency ;
                           owl:someValuesFrom xsd:string
                         ] .


###  https://w3id.org/cocoon/def#NetworkStorage
:NetworkStorage rdf:type owl:Class ;
                rdfs:subClassOf :Network ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasCapacity ;
                                  owl:someValuesFrom :PhysicalQuantity
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasProtocol ;
                                  owl:someValuesFrom :Protocol
                                ] .


###  https://w3id.org/cocoon/def#OperatingSystem
:OperatingSystem rdf:type owl:Class ;
                 rdfs:subClassOf :SoftwareType .


###  https://w3id.org/cocoon/def#Operation
:Operation rdf:type owl:Class .


###  https://w3id.org/cocoon/def#Output
:Output rdf:type owl:Class ;
        rdfs:subClassOf :Operation .


###  https://w3id.org/cocoon/def#PaaS
:PaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService .


###  https://w3id.org/cocoon/def#Password
:Password rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hash ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] .


###  https://w3id.org/cocoon/def#PhysicalQuantity
:PhysicalQuantity rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasUnitOfMeasurement ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :UnitOfMeasure
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :numericValue ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:double
                                  ] .


###  https://w3id.org/cocoon/def#PrivateCloud
:PrivateCloud rdf:type owl:Class ;
              rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/def#Protocol
:Protocol rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :protocolVersion ;
                            owl:someValuesFrom xsd:string
                          ] .


###  https://w3id.org/cocoon/def#Provider
:Provider rdf:type owl:Class ;
          rdfs:subClassOf :ServiceLevelAgreement ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :URI ;
                            owl:someValuesFrom xsd:anyURI
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :providerDescription ;
                            owl:someValuesFrom rdfs:Literal
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :providerName ;
                            owl:someValuesFrom xsd:string
                          ] .


###  https://w3id.org/cocoon/def#PublicCloud
:PublicCloud rdf:type owl:Class ;
             rdfs:subClassOf :DeploymentModel .


###  https://w3id.org/cocoon/def#QualityOfService
:QualityOfService rdf:type owl:Class .


###  https://w3id.org/cocoon/def#RDP
:RDP rdf:type owl:Class ;
     rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#REST
:REST rdf:type owl:Class ;
      rdfs:subClassOf :Interface ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :method ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :requestPayload ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :responsePayload ;
                        owl:someValuesFrom xsd:string
                      ] .


###  https://w3id.org/cocoon/def#RSync
:RSync rdf:type owl:Class ;
       rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf :Location .


###  https://w3id.org/cocoon/def#SCP
:SCP rdf:type owl:Class ;
     rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#SSH
:SSH rdf:type owl:Class ;
     rdfs:subClassOf :Protocol .


###  https://w3id.org/cocoon/def#SaaS
:SaaS rdf:type owl:Class ;
      rdfs:subClassOf :CloudService ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isSoftware ;
                        owl:someValuesFrom :Software
                      ] .


###  https://w3id.org/cocoon/def#Script
:Script rdf:type owl:Class ;
        rdfs:subClassOf :SoftwareType .


###  https://w3id.org/cocoon/def#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasOrchestrationInterface ;
                           owl:someValuesFrom :Interface
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasServiceLevelAgreement ;
                           owl:someValuesFrom :ServiceLevelAgreement
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSupportedProtocol ;
                           owl:someValuesFrom :Protocol
                         ] .


###  https://w3id.org/cocoon/def#ServiceLevelAgreement
:ServiceLevelAgreement rdf:type owl:Class .


###  https://w3id.org/cocoon/def#Software
:Software rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasFunctionality ;
                            owl:someValuesFrom :SoftwareFunctionality
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isSoftwareType ;
                            owl:someValuesFrom :SoftwareType
                          ] .


###  https://w3id.org/cocoon/def#SoftwareFunctionality
:SoftwareFunctionality rdf:type owl:Class .


###  https://w3id.org/cocoon/def#SoftwareType
