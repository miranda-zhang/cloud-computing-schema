###  https://w3id.org/cocoon/def#VM
:VM rdf:type owl:Class ;
    rdfs:subClassOf :IaaS ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasCPU ;
                      owl:someValuesFrom :CPU
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasCPUcapacity ;
                      owl:someValuesFrom :PhysicalQuantity
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasIOConfiguration ;
                      owl:someValuesFrom :Attribute
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasMemory ;
                      owl:someValuesFrom :PhysicalQuantity
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasNetworkStorage ;
                      owl:someValuesFrom :Storage
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasOS ;
                      owl:someValuesFrom :OperatingSystem
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :hasVirtualization ;
                      owl:someValuesFrom :Virtualization
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :numberOfCores ;
                      owl:someValuesFrom xsd:decimal
                    ] .


