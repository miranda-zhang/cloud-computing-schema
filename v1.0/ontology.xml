    <!-- https://w3id.org/cocoon/v1.0#CloudService -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#CloudService">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cocoon/v1.0#Service"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/goodrelations/v1#hasPriceSpecification"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/goodrelations/v1#UnitPriceSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#hasProvider"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/goodrelations/v1#BusinessEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://w3id.org/cocoon/v1.0#CloudService"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/goodrelations/v1#hasPriceSpecification"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/goodrelations/v1#UnitPriceSpecification"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">Price specification for Cloud Service</rdfs:comment>
    </owl:Axiom>
    


