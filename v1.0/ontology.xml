    <!-- https://w3id.org/cocoon/v1.0#bandwidth -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#bandwidth"/>
    


    <!-- https://w3id.org/cocoon/v1.0#chargedPerCore -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#chargedPerCore">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment xml:lang="en">For Google Cloud, cost of SQL server image is charged on per core basis.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#continent -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#continent">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/owl/owlapi/turtle#location"/>
        <rdfs:comment xml:lang="en">Similar to location, this property specifies the continent.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#forCoresLessEqual -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#forCoresLessEqual">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment xml:lang="en">Google Cloud OS price for VM with different number of cores are different.
It is used to describe price for machines with less than or
equal to the specified number of cores.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#forCoresMoreThan -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#forCoresMoreThan">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment xml:lang="en">Google Cloud OS price for VM with different number of cores are different.
This is used to describe the price for machines with more than specified
number of cores.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#latency -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#latency"/>
    


    <!-- https://w3id.org/cocoon/v1.0#location -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#location">
        <rdfs:comment xml:lang="en">The human-readable name for the actual location of the data center in a region, this is the original value used to generate the linking to Geoname ontology.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#numberOfCores -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cocoon/v1.0#numberOfCores">
        <rdfs:comment>For Google Cloud, cores and vCPU referes to the same thing.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cocoon/v1.0#numericValue -->


