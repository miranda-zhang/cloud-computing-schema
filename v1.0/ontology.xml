     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/cocoon/v1.0">
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2012-10-01</terms:created>
        <terms:creator rdf:resource="https://w3id.org/cocoon/v1.0#mirandazhangqian"/>
        <terms:description xml:lang="en">Simple ontology for Cloud Computing Services. This ontology allows to define model of prices used in large cloud computing providers such as Google, Amazon, Azure, etc., including options for regions, type of instances, prices specification, etc. </terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-09-20</terms:issued>
        <terms:modified>2018-09-17</terms:modified>
        <terms:rights>Copyright © 2018 Miranda ZHANG Qian</terms:rights>
        <terms:title xml:lang="en">Vocabulary in Cloud Computing Services</terms:title>
        <vann:preferredNamespacePrefix>cocoon</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/cocoon/v1.0#</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">Cloud Computing Services Ontology</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://github.com/miranda-zhang/cloud-computing-schema"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    </owl:Ontology>
    


