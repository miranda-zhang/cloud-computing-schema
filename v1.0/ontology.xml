    <!-- https://w3id.org/cocoon/v1.0#Location -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#Location"/>
    


    <!-- https://w3id.org/cocoon/v1.0#Network -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#Network">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cocoon/v1.0#IaaS"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#hasBandwidth"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/cocoon/v1.0#QuantityOfThings"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#bandwidth"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#latency"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://w3id.org/cocoon/v1.0#NetworkStorage -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#NetworkStorage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cocoon/v1.0#Storage"/>
        <rdfs:comment xml:lang="en">Storage can exist independent from VM</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/cocoon/v1.0#OSPriceSpecification -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#OSPriceSpecification">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#UnitPriceSpecification"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#chargedPerCore"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#forCoresLessEqual"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#forCoresMoreThan"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/cocoon/v1.0#StorageTransactionsPriceSpecification"/>
    </owl:Class>
    


    <!-- https://w3id.org/cocoon/v1.0#PaaS -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#PaaS">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cocoon/v1.0#CloudService"/>
        <rdfs:comment xml:lang="en">Platform as a Service</rdfs:comment>
    </owl:Class>
    


