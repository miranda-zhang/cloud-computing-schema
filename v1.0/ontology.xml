    <!-- https://w3id.org/cocoon/v1.0#QualityOfService -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#QualityOfService"/>
    


    <!-- https://w3id.org/cocoon/v1.0#QuantityOfThings -->


    <owl:Class rdf:about="https://w3id.org/cocoon/v1.0#QuantityOfThings">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#hasUnitOfMeasurement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/cocoon/v1.0#UnitOfMeasure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#ofObjectType"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cocoon/v1.0#numericValue"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#decimal"/>
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#double"/>
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#int"/>
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#integer"/>
                        </owl:unionOf>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
